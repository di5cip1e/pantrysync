# Test Environment Configuration
NODE_ENV=test
EXPO_PUBLIC_ENV=test

# Version Tracking
EXPO_PUBLIC_APP_VERSION=1.0.0-test
EXPO_PUBLIC_BUILD_NUMBER=1

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3001/api
EXPO_PUBLIC_API_TIMEOUT=5000

# Firebase Configuration (Test/Mock)
EXPO_PUBLIC_FIREBASE_API_KEY=test-api-key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=test.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=test-project
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=test-project.firebasestorage.app
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
EXPO_PUBLIC_FIREBASE_APP_ID=test-app-id
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=test-measurement-id

# Feature Flags (Test)
EXPO_PUBLIC_FEATURE_AI_INVENTORY=false
EXPO_PUBLIC_FEATURE_ANALYTICS=false
EXPO_PUBLIC_FEATURE_PUSH_NOTIFICATIONS=false
EXPO_PUBLIC_FEATURE_OFFLINE_MODE=false
EXPO_PUBLIC_FEATURE_DEBUG_MODE=true

# Analytics Configuration
EXPO_PUBLIC_ANALYTICS_ENABLED=false
EXPO_PUBLIC_ANALYTICS_SAMPLE_RATE=0.0

# Cache Configuration
EXPO_PUBLIC_CACHE_ENABLED=false
EXPO_PUBLIC_CACHE_TTL=60000
EXPO_PUBLIC_CACHE_MAX_SIZE=10485760

# Error Reporting
EXPO_PUBLIC_ERROR_REPORTING_ENABLED=false
EXPO_PUBLIC_ERROR_SAMPLE_RATE=0.0

# User Configuration Limits (Test)
EXPO_PUBLIC_MAX_HOUSEHOLDS_PER_USER=5
EXPO_PUBLIC_MAX_ITEMS_PER_HOUSEHOLD=100
EXPO_PUBLIC_MAX_MEMBERS_PER_HOUSEHOLD=10
EXPO_PUBLIC_MAX_SHOPPING_LISTS=5
EXPO_PUBLIC_MAX_FILE_UPLOAD_SIZE=1048576

# Security Configuration (Test)
EXPO_PUBLIC_SESSION_TIMEOUT=600000
EXPO_PUBLIC_MAX_LOGIN_ATTEMPTS=3
EXPO_PUBLIC_PASSWORD_MIN_LENGTH=3

# Development/Testing
EXPO_PUBLIC_DEBUG_LOGS=true
EXPO_PUBLIC_MOCK_DATA=true
EXPO_PUBLIC_TEST_MODE=true