# Staging Environment Configuration
NODE_ENV=staging
EXPO_PUBLIC_ENV=staging

# Version Tracking
EXPO_PUBLIC_APP_VERSION=1.0.0-rc
EXPO_PUBLIC_BUILD_NUMBER=1

# API Configuration
EXPO_PUBLIC_API_URL=https://staging-api.pantrysync.com
EXPO_PUBLIC_API_TIMEOUT=15000

# Firebase Configuration (Staging)
EXPO_PUBLIC_FIREBASE_API_KEY=STAGING_API_KEY_HERE
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=pantrysync-staging.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=pantrysync-staging
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=pantrysync-staging.firebasestorage.app
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=STAGING_SENDER_ID
EXPO_PUBLIC_FIREBASE_APP_ID=STAGING_APP_ID
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=STAGING_MEASUREMENT_ID

# Feature Flags (Staging)
EXPO_PUBLIC_FEATURE_AI_INVENTORY=true
EXPO_PUBLIC_FEATURE_ANALYTICS=true
EXPO_PUBLIC_FEATURE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_FEATURE_OFFLINE_MODE=true
EXPO_PUBLIC_FEATURE_DEBUG_MODE=true

# Analytics Configuration
EXPO_PUBLIC_ANALYTICS_ENABLED=true
EXPO_PUBLIC_ANALYTICS_TRACKING_ID=STAGING_TRACKING_ID
EXPO_PUBLIC_ANALYTICS_SAMPLE_RATE=1.0

# Cache Configuration
EXPO_PUBLIC_CACHE_ENABLED=true
EXPO_PUBLIC_CACHE_TTL=300000
EXPO_PUBLIC_CACHE_MAX_SIZE=52428800

# Error Reporting
EXPO_PUBLIC_ERROR_REPORTING_ENABLED=true
EXPO_PUBLIC_SENTRY_DSN=https://staging-sentry-dsn@sentry.io/project
EXPO_PUBLIC_ERROR_SAMPLE_RATE=1.0

# User Configuration Limits (Production-like)
EXPO_PUBLIC_MAX_HOUSEHOLDS_PER_USER=10
EXPO_PUBLIC_MAX_ITEMS_PER_HOUSEHOLD=1000
EXPO_PUBLIC_MAX_MEMBERS_PER_HOUSEHOLD=20
EXPO_PUBLIC_MAX_SHOPPING_LISTS=10
EXPO_PUBLIC_MAX_FILE_UPLOAD_SIZE=5242880

# Security Configuration (Production-like)
EXPO_PUBLIC_SESSION_TIMEOUT=3600000
EXPO_PUBLIC_MAX_LOGIN_ATTEMPTS=5
EXPO_PUBLIC_PASSWORD_MIN_LENGTH=6

# Development/Testing
EXPO_PUBLIC_DEBUG_LOGS=true
EXPO_PUBLIC_MOCK_DATA=false
EXPO_PUBLIC_TEST_MODE=false